  "components": {
    "schemas": {
      "TeamSetupDataDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          }
        },
        "required": [
          "name"
        ]
      },
      "CreateGameDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "teams": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TeamSetupDataDTO"
            }
          }
        },
        "required": [
          "gameID",
          "teams"
        ]
      },
      "ShipStatsDataDTO": {
        "type": "object",
        "properties": {
          "moveFactor": {
            "type": "number"
          },
          "attackFactor": {
            "type": "number"
          },
          "defenseFactor": {
            "type": "number"
          },
          "attackRange": {
            "type": "number"
          }
        },
        "required": [
          "moveFactor",
          "attackFactor",
          "defenseFactor",
          "attackRange"
        ]
      },
      "ShipSetupDataDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "shipType": {
            "type": "string",
            "enum": [
              "SCOUT",
              "DESTROYER",
              "CRUISER",
              "BATTLESHIP"
            ]
          },
          "stats": {
            "$ref": "#/components/schemas/ShipStatsDataDTO"
          }
        },
        "required": [
          "name"
        ]
      },
      "TeamFleetDataDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "teamName": {
            "type": "string"
          },
          "ships": {
            "description": "List of ships that belong to this team",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ShipSetupDataDTO"
            }
          }
        },
        "required": [
          "gameID",
          "teamName",
          "ships"
        ]
      },
      "StartGameDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "turnLength": {
            "type": "number"
          }
        },
        "required": [
          "gameID",
          "turnLength"
        ]
      },
      "GetCubeStateDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "teamName": {
            "type": "string"
          }
        },
        "required": [
          "gameID",
          "teamName"
        ]
      },
      "GetNeighborsDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          }
        },
        "required": [
          "gameID"
        ]
      },
      "VertexDTO": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number"
          },
          "y": {
            "type": "number"
          },
          "z": {
            "type": "number"
          }
        },
        "required": [
          "x",
          "y",
          "z"
        ]
      },
      "GetVertexDataDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "teamName": {
            "type": "string"
          },
          "vertex": {
            "$ref": "#/components/schemas/VertexDTO"
          }
        },
        "required": [
          "gameID",
          "teamName",
          "vertex"
        ]
      },
      "EdgeDTO": {
        "type": "object",
        "properties": {
          "v1": {
            "$ref": "#/components/schemas/VertexDTO"
          },
          "v2": {
            "$ref": "#/components/schemas/VertexDTO"
          }
        },
        "required": [
          "v1",
          "v2"
        ]
      },
      "GetEdgeDataDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "teamName": {
            "type": "string"
          },
          "edge": {
            "$ref": "#/components/schemas/EdgeDTO"
          }
        },
        "required": [
          "gameID",
          "teamName",
          "edge"
        ]
      },
      "ShipIdentifierDTO": {
        "type": "object",
        "properties": {
          "teamName": {
            "type": "string"
          },
          "shipName": {
            "type": "string"
          }
        },
        "required": [
          "teamName",
          "shipName"
        ]
      },
      "MoveShipDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "direction": {
            "type": "string",
            "enum": [
              "CURRENT",
              "UP",
              "DOWN",
              "LEFT",
              "RIGHT",
              "FORWARD",
              "BACK"
            ]
          },
          "movingShip": {
            "$ref": "#/components/schemas/ShipIdentifierDTO"
          }
        },
        "required": [
          "gameID",
          "direction",
          "movingShip"
        ]
      },
      "AttackDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "direction": {
            "type": "string",
            "enum": [
              "CURRENT",
              "UP",
              "DOWN",
              "LEFT",
              "RIGHT",
              "FORWARD",
              "BACK"
            ]
          },
          "attackingShip": {
            "$ref": "#/components/schemas/ShipIdentifierDTO"
          },
          "target": {
            "$ref": "#/components/schemas/ShipIdentifierDTO"
          }
        },
        "required": [
          "gameID",
          "direction",
          "attackingShip",
          "target"
        ]
      },
      "CaptureDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          },
          "direction": {
            "type": "string",
            "enum": [
              "CURRENT",
              "UP",
              "DOWN",
              "LEFT",
              "RIGHT",
              "FORWARD",
              "BACK"
            ]
          },
          "capturingShip": {
            "$ref": "#/components/schemas/ShipIdentifierDTO"
          }
        },
        "required": [
          "gameID",
          "direction",
          "capturingShip"
        ]
      },
      "ResetGameDTO": {
        "type": "object",
        "properties": {
          "gameID": {
            "type": "string"
          }
        },
        "required": [
          "gameID"
        ]
      }
    }
  }
}
